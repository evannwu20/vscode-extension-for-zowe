{
    "name": "vscode-extension-for-zowe",
    "displayName": "%package.displayName%",
    "description": "%package.description%",
    "version": "0.18.0",
    "publisher": "Zowe",
    "repository": {
        "url": "https://github.com/zowe/vscode-extension-for-zowe"
    },
    "icon": "resources/zowe_logo.png",
    "keywords": [
        "mainframe",
        "zos",
        "dataset",
        "zowe"
    ],
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
