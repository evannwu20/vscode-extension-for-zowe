        "menus": {
            "view/title": [
                {
                    "when": "view == zowe.explorer",
                    "command": "zowe.refreshAll",
                    "group": "navigation"
                },
                {
                    "when": "view == zowe.explorer",
                    "command": "zowe.addSession",
                    "group": "navigation"
                },
                {
                    "when": "view == zowe.uss.explorer",
                    "command": "zowe.uss.refreshAll",
                    "group": "navigation"
                },
                {
                    "when": "view == zowe.uss.explorer",
                    "command": "zowe.uss.addSession",
                    "group": "navigation"
                },
                {
                    "when": "view == zowe.jobs",
